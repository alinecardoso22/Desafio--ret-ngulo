Algoritmo "Retângulo"

Var

base, altura : inteiro
diagonal: real

Inicio

 escreva("Digite o valor da base do retângulo: ")
  leia(base)
 escreva("Digite o valor da altura do retângulo: ")
  leia(altura)
  
 diagonal <- raizq ( exp(base,2) + exp(altura,2) )

 escreva("DIAGONAL = ",diagonal:2:2)

Fimalgoritmo